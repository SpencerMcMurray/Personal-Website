    "bootstrap": "^4.3.1",
    "concurrently": "^3.5.1",
    "express": "^4.16.2",
    "react-animate-on-scroll": "^2.1.5",
    "react-bootstrap": "^1.0.0-beta.5",
    "react-router-dom": "^5.0.0",
    "react-scrollable-anchor": "^0.6.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.4"
  }
